<?xml version="1.0" encoding="ISO-8859-1"?>
<illumination_settings>

  <control_modules>

    <control_module>
      <name>AOTF</name>
      <module_name>sc_hardware.crystalTechnologies.hAotf</module_name>
      <class_name>CrystalTechAOTF64Bit</class_name>
      <parameters>
	<use_fsk type="boolean">true</use_fsk>
      </parameters>
    </control_module>
    
    <control_module>
      <name>cube405</name>
      <module_name>sc_hardware.coherent.hLaser</module_name>
      <class_name>CoherentCube</class_name>
      <parameters>
	<port>COM3</port>
      </parameters>
    </control_module>

    <control_module>
      <name>cube445</name>
      <module_name>sc_hardware.coherent.hLaser</module_name>
      <class_name>CoherentCube</class_name>
      <parameters>
	<port>COM5</port>
      </parameters>
    </control_module>
    
    <control_module>
      <name>daq</name>
      <module_name>sc_hardware.nationalInstruments.hNicontrol</module_name>
      <class_name>Nidaq</class_name>
      <parameters>
	<counter_board>PCI-6722</counter_board>
	<counter_id type="int">0</counter_id>
	<counter_trigger type="int">0</counter_trigger>
	<waveform_clock>ctr0out</waveform_clock>
      </parameters>
    </control_module>
  </control_modules>

  <channels>

    <!-- 640 laser -->
    <channel>
      <description>640</description>
      <color type="rgb">255,0,0</color>
      
      <amplitude_modulation>
	<uses>AOTF</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">7600</maximum>
	  <channel type="int">0</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">91.47</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">0</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">0</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 561 laser -->
    <channel>
      <description>561</description>
      <color type="rgb">255,255,0</color>
      
      <amplitude_modulation>
	<uses>AOTF</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">6400</maximum>
	  <channel type="int">1</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">107.71</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">1</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">1</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 532 laser -->
    <channel>
      <description>532</description>
      <color type="rgb">0,255,0</color>
      
      <amplitude_modulation>
	<uses>AOTF</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">6500</maximum>
	  <channel type="int">2</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">115.62</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">2</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">2</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 514 laser -->
    <channel>
      <description>514</description>
      <color type="rgb">0,255,100</color>
      
      <amplitude_modulation>
	<uses>AOTF</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">6400</maximum>
	  <channel type="int">3</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">121.53</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">3</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">3</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 488 laser -->
    <channel>
      <description>488</description>
      <color type="rgb">0,255,255</color>
      
      <amplitude_modulation>
	<uses>AOTF</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">5800</maximum>
	  <channel type="int">4</channel>
	  <off_frequency type="float">20.0</off_frequency>
	  <on_frequency type="float">130.52</on_frequency>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">4</channel>
	  <max_voltage type="float">1.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">4</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 445 laser -->
    <channel>
      <description>445</description>
      <color type="rgb">100,0,255</color>

      <amplitude_modulation>
	<uses>cube445</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">4000</maximum>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">5</channel>
	  <max_voltage type="float">5.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">5</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- 405 laser -->
    <channel>
      <description>405</description>
      <color type="rgb">255,0,255</color>

      <amplitude_modulation>
	<uses>cube405</uses>
	<display_normalized type="boolean">True</display_normalized>
	<parameters>
	  <maximum type="int">5000</maximum>
	</parameters>
      </amplitude_modulation>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">6</channel>
	  <max_voltage type="float">5.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

      <mechanical_shutter>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">6</channel>
	</parameters>
      </mechanical_shutter>

    </channel>

    <!-- Lamp -->
    <channel>
      <description>White</description>
      <color type="rgb">255,255,255</color>
      
      <analog_modulation>
	<uses>daq</uses>
	<parameters>
	  <board>PCI-6722</board>
	  <channel type="int">7</channel>
	  <max_voltage type="float">5.0</max_voltage>
	  <min_voltage type="float">0.0</min_voltage>
	</parameters>
      </analog_modulation>

    </channel>


  </channels>

</illumination_settings>
